From f35f5b174f018adc2d271e9e0c16382a07feba94 Mon Sep 17 00:00:00 2001
From: Derek Higgins <derekh@redhat.com>
Date: Thu, 16 May 2013 21:31:15 +0100
Subject: [PATCH] No longer start nova-rpc-zmq-receiver

Fixes bug 1179860

Its not used and fails to start
---
 elements/nova-api/install.d/80-nova-api                  |    1 -
 elements/nova/os-refresh-config/post-configure.d/74-nova |    1 -
 2 files changed, 2 deletions(-)

diff --git a/elements/nova-api/install.d/80-nova-api b/elements/nova-api/install.d/80-nova-api
index 941bb5b..c28995b 100755
--- a/elements/nova-api/install.d/80-nova-api
+++ b/elements/nova-api/install.d/80-nova-api
@@ -2,7 +2,6 @@
 set -eux
 
 os-svc-daemon nova-api              nova nova-api              "--config-dir /etc/nova"
-os-svc-daemon nova-rpc-zmq-receiver nova nova-rpc-zmq-receiver "--config-dir /etc/nova"
 os-svc-daemon nova-cert             nova nova-cert             "--config-dir /etc/nova"
 os-svc-daemon nova-scheduler        nova nova-scheduler        "--config-dir /etc/nova --debug"
 os-svc-daemon nova-consoleauth      nova nova-consoleauth      "--config-dir /etc/nova"
diff --git a/elements/nova/os-refresh-config/post-configure.d/74-nova b/elements/nova/os-refresh-config/post-configure.d/74-nova
index 4d71162..76bb614 100755
--- a/elements/nova/os-refresh-config/post-configure.d/74-nova
+++ b/elements/nova/os-refresh-config/post-configure.d/74-nova
@@ -13,7 +13,6 @@ chown -R nova:nova /tftpboot
 service nova-conductor restart
 service nova-api restart
 service nova-cert restart
-service nova-rpc-zmq-receiver restart
 service nova-compute restart
 service nova-scheduler restart
 service nova-consoleauth restart
-- 
